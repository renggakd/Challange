<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rengga.challange">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Challange">
        <activity android:name=".menu.MenuActivity"></activity>
        <activity android:name=".main.VsPlayerActivity" />
        <activity android:name=".main.VsComputerActivity" />
        <activity android:name=".fragment.FragmentActivity" />

        <activity android:name=".ui.splashscreen.SplashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.rengga.challange.main.VsComputerActivity" />
        <activity android:name="com.rengga.challange.main.VsPlayerActivity" />
    </application>

</manifest>